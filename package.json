{
  "name": "mongo-registry",
  "version": "1.0.4",
  "description": "Gère les montée de version du schéma mongo au niveau applicatif.",
  "main": "index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive --require babel-register --exit \"test/**.js\"",
    "build": "rimraf dist && yarn run test && babel src -d dist",
    "prepublishOnly": "npm version patch && npm run build",
    "npmpublish": "npm publish"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "compare-versions": "3.3.0",
    "fraction.js": "4.0.4",
    "lodash": "4.17.5",
    "mongo-connexion": "1.1.1",
    "test-api-express-mongo": "1.0.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-plugin-transform-builtin-extend": "1.1.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-3": "6.24.1",
    "chai": "4.1.2",
    "cross-env": "5.2.0",
    "mocha": "5.0.3",
    "rimraf": "2.6.2",
    "yarn": "1.7.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-3"
    ],
    "plugins": [
      "transform-runtime",
      [
        "transform-builtin-extend",
        {
          "globals": [
            "Error",
            "Array"
          ]
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": []
      }
    }
  }
}
